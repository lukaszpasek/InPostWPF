<Window x:Class="InPost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodels="clr-namespace:InPost.ViewModels" xmlns:local="clr-namespace:InPost.View" d:DataContext="{d:DesignInstance Type=viewmodels:MainViewModel}"
        mc:Ignorable="d"
        Title="MainWindow" Height="600" Width="1080">
    <ItemsControl ItemsSource="{Binding SiecPaczkomatow}" Margin="65,89,104,66">
        <ItemsControl.ItemTemplate>
            <DataTemplate>
                <StackPanel>
                    <local:Paczkomat />
                </StackPanel>
            </DataTemplate>
        </ItemsControl.ItemTemplate>
    </ItemsControl>
</Window>
